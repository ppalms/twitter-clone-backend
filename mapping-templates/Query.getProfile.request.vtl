{
    "version" : "2018-05-29",
    "operation" : "Query",
    "query" : {
        "expression" : "screenName = :screenName",
        "expressionValues" : {
            ":screenName" : $util.dynamodb.toDynamoDBJson($context.arguments.screenName)
        }
    },
    "index" : "byScreenName",
    "limit" : 1,
    "consistentRead" : false,
    "select" : "ALL_ATTRIBUTES"
}